                                <% logs.forEach(log => { %>
                               <tr>
                                   
                                   <td><%= log.MSISDN %> </td>
                                   <td><%= log.Manufacturer %> </td>
                                   <td><%= log.Model %> </td>
                                   <td><%= log.Release_Date %> </td>
                                   <td><%= log.GPRS %> </td>
                                   <td><%= log.EDGE %> </td>
                                   <td><%= log.UMTS %> </td>
                                   <td><%= log.HSDPA %> </td>
                                   <td><%= log.LTE %> </td>
                                   <td><%= log.Device_Type %> </td>
                               </tr>
                                <% }) %>
                                 <% } %> 
                            
                            
                        </tbody>
                        <tfoot>
                            <tr>
                               
                            </tr>
                        </tfoot>
                    </table>
