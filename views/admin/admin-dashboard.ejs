                    </div>
                </div>
                <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                    <table class="table my-0" id="dataTable">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">First Name</th>
                                <th scope="col">Last Name</th>
                                <th scope="col">UserName</th>
                                <th scope="col">email</th>
                                <th scope="col">MSISDN</th>
                                <th scope="col">User Level</th>
                                <th scope="col" colspan="2">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if (typeof users !='undefined' ) { %>
                                <% for( let index=0; index < users.length; index++ ) { %>
                            <tr>
                                <td scope="row">
                                    <%- (index+1) %>
                                </td>
                                <td>
                                    <%- users[index].firstname %>
                                </td>
                                <td>
                                    <%- users[index].lastname %>
                                </td>
                                <td>
                                    <%- users[index].username %>
                                </td>
                                <td>
                                    <%- users[index].email %>
                                </td>
                                <td>
                                    <%- users[index].number %>
                                </td>
                                <td>
                                    <%- users[index].userLevel %>
                                </td>
                                <td scope="row"><a href="javascript:void(0)"
                                    onclick="location.href='/admin/admin-dashboard/delete/<%- users[index].userId %>'"
                                    class="link text-danger">
                                    <i class="fa fa-trash text-danger text-center fa-lg"
                                        aria-hidden="true"></i></a>
                                </td>
                                <td scope="row"><a href="" data-toggle="modal" data-target="#staticBackdrop"
                                    class="link text-danger">
                                    <i class="fa fa-edit text-primary text-center fa-lg"
                                        aria-hidden="true"></i></a>
                            </td>
                            </tr>
                            
                            
                            <% } %>
                                        <% } %>
                            
                            
                        </tbody>
                        <tfoot>
                            <tr>
                               
                            </tr>
                        </tfoot>
